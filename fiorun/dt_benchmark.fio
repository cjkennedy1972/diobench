; fio bench mark with 4 jobs on 4 separate devices 

[global]
randrepeat=0
ioengine=libaio
direct=1
refill_buffers
numjobs=1
runtime=28800
group_reporting
time_based
blocksize=4k
rwmixwrite=60
rw=randrw
name=benchtest

[job-1]
filename=/dev/vdb
numjobs=2
iodepth=8

[job-2]
filename=/dev/vdc
numjobs=2
iodepth=8

[job-3]
filename=/dev/vdd
numjobs=2
iodepth=8

[job-4]
filename=/dev/vde
numjobs=2
iodepth=8


